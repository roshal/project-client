{
	"hosting": {
		"public": "public",
		"rewrites": [
			{
				"source": "**",
				"destination": "/source.html"
			}
		],
		"headers": [
			{
				"source": "/@(favicon.png|@(fonts|images)/**)",
				"headers": [
					{
						"key": "Cache-Control",
						"value": "max-age=604800"
					}
				]
			}
		]
	}
}
